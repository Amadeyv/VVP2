#include <stdio.h>
#include <math.h>

int main() {
    int n, v, num1, num2, v1, v2;
    printf("vvedite n = ");
    scanf("%d", &n);
    int a[n];
    v = 0;
    while (v<n) {
        printf("a[%d] = ", v);
        scanf("%d", &a[v]);
        v = v + 1;
    }
    v1 = 0;
    while (v1<n-1) {
        v2 = v1 + 1;
        while (v2<n) {
            if (a[v1]==a[v2]) {
                num1 = v1;
                num2 = v2;
                break;
            }
            else
                v2++;
        }
        v1++;
    }
    printf("num1 = %d, num2 = %d", (num1+1), (num2+1));
    return 0;
}
