#include <stdio.h>

int main() {
    int v1, v2, n;
    printf("vvedite n = ");
    scanf("%d", &n);
    int a[n];
    v1 = 0;
    while (v1<n) {
        printf("a[%d] = ", v1);
        scanf("%d", &a[v1]);
        v1++;
    }
    v1 = 0;
    while (v1<n-1) {
        v2 = v1 + 1; 
        while (v2<n) {
            if (a[v1]==a[v2]) {
                for (int i = v1; i < n-1; i++) {
                    a[i] = a[i + 1];
                }
                n--;
                for (int i = v2-1; i < n-1; i++) {
                    a[i] = a[i+1];
                }
                n--;
            }
            v2++;
        }    
        v1++;
    }
    v1 = 0;
    while (v1<n) {
        printf("a[%d] = %d\n", v1, a[v1]);
        v1++;
    }
    printf("razmer massiva n = %d", n);
    return 0;
}
