#include <stdio.h>

int main() {
    int v, n;
    printf("vvedite n = ");
    scanf("%d", &n);
    int a[n];
    v = 0;
    while (v<n) {
        printf("a[%d] = ", v);
        scanf("%d", &a[v]);
        v++;
    }
    v = 1;
    while (v<n) {
        if (a[v-1]==a[v]) {
            for (int i = v; i < n-1; i++) {
                a[i] = a[i + 1];
            }
            n--;
        }
        v++;
    }
    v = 0;
    while (v<n) {
        printf("a[%d] = %d\n", v, a[v]);
        v++;
    }
    return 0;
}
