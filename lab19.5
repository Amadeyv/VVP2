#include <stdio.h>

int main() {
    int n, v, p;
    printf("vvedite n = ");
    scanf("%d", &n);
    int a[n];
    v = 0;
    while (v<n) {
        printf("a[%d] = ", v);
        scanf("%d", &a[v]);
        v++;
    }
    v = 0;
    while (v<n) {
        if (a[v]>0) {
            n++;
            p = n-1;
            while (p>v) {
                a[p] = a[p-1];
                p--;
            }
            a[v] = 0;
            v += 2;
        }
        else
            v++;
    }
    v = 0;
    while (v<n) {
        printf("[%d] ", a[v]);
        v++;
    }
    return 0;
}
