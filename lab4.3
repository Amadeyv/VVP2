#include <stdio.h>

int main() {
    int a, b, s, r, p;
    float ch, a2, b2;
    printf("Chisla a = ");
    scanf("%d/n", &a);
    printf("b = ");
    scanf("%d/n", &b);
    a2 = a*a;
    b2 = b*b;
    s = a*a + b*b;
    r = a*a - b*b;
    p = a*a*b*b;
    ch = a2/b2;
    printf("summa s = %d  ", s);
    printf("raznost r = %d  ", r);
    printf("proizvedenie p = %d  ", p);
    printf("chastnoe ch = %f  ", ch);
    return(0);
}
