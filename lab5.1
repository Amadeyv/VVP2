#include <stdio.h>

int main() {
    float r;
    int  x1, y1, x2, y2, vx, vy;
    printf("vvedite koordinati x1 = ");
    scanf("%d", &x1);
    printf("  y1 = ");
    scanf("%d", &y1);
    printf("  x2 = ");
    scanf("%d", &x2);
    printf("  y2 = ");
    scanf("%d", &y2);
    vx = x2 - x1;
    vy = y2 - y1;
    r = sqrt((vx*vx) + (vy*vy));
    printf("rasstoyanie r = %f", r);
    return 0;
}
