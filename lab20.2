#include <stdio.h>

int main() {
    int n, v, k, r, p, q, l;
    printf("vvedite n = ");
    scanf("%d", &n);
    printf("vvedite l = ");
    scanf("%d", &l);
    int a[n];
    v = 0;
    while (v<n) {
        printf("a[%d] = ", v);
        scanf("%d", &a[v]);
        v++;
    }
    r = 0;
    int b[r+1], c[r+1];
    k = 1;
    v = 1;
    while (v<n) {
        if (a[v]==a[v-1]) {
            if (v==n-1) {
                k++;
                b[r] = k;
                c[r] = a[v];
            }
            else
                k++;
        }
        else {
            b[r] = k;
            c[r] = a[v-1];
            r++;
            k = 1;
        }
        v++;
    }
    v = 0;
    while (v<r+1) {
        if (b[v]>l) {
            k = 0;
            while (k<n) {
                if (a[k]==c[v]) {
                    a[k+b[v]-1] = 0;
                    p = k;
                    while (p<n-1) {
                        a[p] = a[p+1];
                        p++;
                    }
                    n--;
                    b[v]--;
                }
                else
                    k++;
            }
        }
        v++;
    }
    printf("a[] = ");
    v = 0;
    while (v<n) {
        printf("[%d] ", a[v]);
        v++;
    }
    return 0;
}
