#include <stdio.h>
#include <math.h>

int main() {
    int n, v, p;
    printf("vvedite n = ");
    scanf("%d", &n);
    int a[n], b[n];
    v = 0;
    while (v<n) {
        printf("a[%d] = ", v);
        scanf("%d", &a[v]);
        v = v + 1;
    }
    v = 0;
    while (v<n) {
        printf("b[%d] = ", v);
        scanf("%d", &b[v]);
        v = v + 1;
    }
    v = 0;
    while (v<n) {
        p = a[v];
        a[v] = b[v];
        b[v] = p;
        printf("a[%d] = %d\n", v, a[v]);
        v++;
    }
    v = 0;
    while (v<n) {
        printf("b[%d] = %d\n", v, b[v]);
        v++;
    }
    return 0;
}
