#include <stdio.h>
#include <math.h>

int main() {
    int v1, v2, s, t, s2;
    printf("skorost v1 = ");
    scanf("%d", &v1);
    printf("skorost v2 = ");
    scanf("%d", &v2);
    printf("rasstoyanie s = ");
    scanf("%d", &s);
    printf("vremya t = ");
    scanf("%d", &t);
    s2 = (v1+v2)*t + s;
    printf("rasstoyanie s2 = %d", s2);
    return(0);
}
